<h1>  Add New List </h1>

<div class="mt-3">
  <%= simple_form_for(@list) do |f| %>
    <%= f.input :name %>
    <%= f.submit "Submit", class: "btn btn-primary mt-3" %>
  <% end %>
</div>

<div>
    <%= link_to lists_path do %>
      <i class="fa-solid fa-plus"></i> Go back to lists
   <% end %>
  </div>
